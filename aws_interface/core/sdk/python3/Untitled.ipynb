      "            \"creationDate\": \"int\",\n",
      "            \"email\": \"str\",\n",
      "            \"passwordHash\": \"str\",\n",
      "            \"salt\": \"str\",\n",
      "            \"group\": \"str\",\n",
      "            \"extra\": \"map\"\n",
      "        }\n",
      "    }\n",
      "}\n",
      "----------------------------API Name: get_user_count----------------------------\n",
      "[SDK Function Call Format]\n",
      "call_api(\"get_user_count\", {})\n",
